<%
const now = new Date()
const style = 'style="text-decoration:line-through;"'
%>
<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <%for (var index in events){ const event = events[index]%>
      <div <% if (now > event.start) {%><%-style%><%}%>>
        <%= event.summary %> am <%= event.start.toLocaleDateString().replace(/([0-9]{2,4})-([0-9]{1,2})-([0-9]{1,2})/,'$3.$2.$1') %>
      </div>
    <%}%>
  </body>
</html>
